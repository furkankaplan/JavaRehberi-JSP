<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>


  <rule>
    <from>^/dersler/([a-zA-Z0-9-]+)/([0-9]+)/([a-zA-Z0-9-]+)?$</from>
    <to>/readEssayPage.jsp?catUrl=$1&amp;essayID=$2&amp;title=$3</to>
  </rule>

  <rule>
    <from>^/test</from>
    <to>/loginPage.jsp</to>
  </rule>

</urlrewrite>